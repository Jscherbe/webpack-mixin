{
  "name": "@ulu/webpack-mixin",
  "version": "1.0.13",
  "description": "Reusable base webpack config for developing traditional sites/themes (Drupal, etc). Includes things commonly needed SASS/LESS, image optimization, extract CSS, local server proxy, etc. Not a SPA setup",
  "main": "index.js",
  "scripts": {
    "serve": "webpack serve --mode development --progress --stats minimal",
    "build": "webpack build --mode production --progress --stats minimal",
    "analyze": "webpack build --mode production --progress --stats minimal --env analyze",
    "watch": "webpack watch --mode production --stats minimal",
    "webpack-help": "webpack --help",
    "debug-configs": "webpack build --mode production --stats minimal --env debugConfigMerge",
    "debug-verbose": "webpack build --mode production --stats verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Jscherbe@github.com/Jscherbe/webpack-mixin.git"
  },
  "keywords": [
    "webpack",
    "sass",
    "scss",
    "less",
    "imagemin",
    "image optimization",
    "webpack-dev-server",
    "webpack.config.js",
    "mixin",
    "autoprefixer",
    "proxy",
    "configuration",
    "config",
    "defaults",
    "preset",
    "mamp",
    "drupal",
    "babel",
    "vue",
    "static site"
  ],
  "author": "Joe Scherben",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jscherbe/webpack-mixin/issues"
  },
  "homepage": "https://github.com/Jscherbe/webpack-mixin#readme",
  "devDependencies": {
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "autoprefixer": "^10.4.1",
    "babel-loader": "^8.2.3",
    "chalk": "^4.1.2",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "image-minimizer-webpack-plugin": "^3.2.1",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "sass": "^1.45.2",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "vue-loader": "^15.9.8",
    "webpack": "^5.65",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
    "webpack-merge": "^5.8.0",
    "webpack-messages": "^2.0.4",
    "yaml-loader": "^0.6.0"
  }
}
